{

    "name": "ulrichanani/eloquent-filter",
    "description": "Allows Dynamic filtering Eloquent Models easily by query string",
    "keywords": [
        "laravel",
        "eloquent",
        "eloquent filter",
        "model",
        "search",
        "query",
        "filter"
    ],
    "license": "MIT",
    "type": "lib",
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.2|^6|^7",
        "illuminate/container": "^5.2|^6|^7",
        "illuminate/database": "^5.2|^6|^7",
        "illuminate/pagination": "^5.2|^6|^7",
        "fzaninotto/faker": "^1.9",
        "morilog/jalali": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*|7.0.*|7.5|8.3|8.5",
        "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*|4.*|4.*|5.*",
        "orchestra/database": "v3.5.0|v3.6.0|v3.7.0|v3.8.0|v4.0.0|v5.0.0|v5.0.0",
        "mockery/mockery": ">=0.9.9"
    },
    "autoload": {
        "psr-4": {
            "eloquentFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit --coverage-text"
    }
}